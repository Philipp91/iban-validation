{
	"name" : "jschaedl/iban-validation",
	"license" : "MIT",
	"type" : "library",
	"description" : "A small library for validating International BankAccount Numbers (IBANs).",
	"keywords" : ["iban", "validation"],
	"homepage" : "http://github.com/jschaedl/iban-validation",
	"authors" : [{
		"name" : "Jan Schaedlich",
		"email" : "mail@janschaedlich.com",
		"homepage" : "https://www.linkedin.com/in/janschaedlich"
	}],
	"autoload": {
		"psr-4": { "Iban\\Validation\\": "" }
	},
	"minimum-stability": "dev",
	"extra": {
		"branch-alias": {
			"dev-master": "1.0-dev"
		}
	},
	"require" : {
		"php" : "^7.1.3",
		"symfony/options-resolver": "^4.2@dev",
		"symfony/yaml": "^4.2@dev"
	},
	"require-dev" : {
		"phpunit/phpunit" : "7.3.1",
		"phpunit/php-code-coverage": "^6.0@dev"
	},
	"suggest": {
       	"ext-bcmath" : "This library makes use of bcmod function, so an installed bcmath extension is recommended."
	}
}
